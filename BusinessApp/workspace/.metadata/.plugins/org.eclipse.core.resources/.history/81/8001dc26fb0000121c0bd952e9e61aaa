package com.fervil.spring.careercoach.model.domain;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;


@Entity
@Table(name = "USER_PROFILE")
public class UserProfile  implements Serializable {
	public UserProfile() {
		
	}	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private long userProfileId;
	private String displayName;
	private int userProfileType;
	private long user_userId;
	private int accountType;
	private String email;
	private String overview;
	private String serviceDescription;
	private String skillsExpertise;
	private String TitleSummayOfhighestPosition;
	private String companyOfHighestPosition;
	private double hourlyRate;
	private String paymentTerms;

	@Id
	@Column(name = "userProfileId", unique = true, nullable = false, precision = 10, scale = 0)
	public long getUserProfileId() {
		return userProfileId;
	}

	public void setUserProfileId(long userProfileId) {
		this.userProfileId = userProfileId;
	}	
	
	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getDisplayName() {
		return displayName;
	}

	public void setDisplayName(String displayName) {
		this.displayName = displayName;
	}	

	@Column(name = "userProfileType", nullable = false)
	public int getUserProfileType (){
		return userProfileType;
	}
	
	userProfileType
	userProfileType
	
	@Column(name = "user_userId", nullable = false)
	public String getUser_userId
	user_userId
	user_userId

	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getAccountType
	accountType
	accountType
	
	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getEmail
	email
	email

	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getOverview
	overview
	overview

	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getServiceDescription
	serviceDescription
	serviceDescription
	
	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getSkillsExpertise
	skillsExpertise
	skillsExpertise
	
	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getTitleSummayOfhighestPosition
	TitleSummayOfhighestPosition
	TitleSummayOfhighestPosition
	
	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getCompanyOfHighestPosition
	companyOfHighestPosition
	companyOfHighestPosition

	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getHourlyRate
	hourlyRate
	hourlyRate

	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getPaymentTerms
	paymentTerms
	paymentTerms

	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getKeywords
	keywords
	keywords

	@Column(name = "disPlayName", nullable = false, length = 75)
	public String getModifiedDate
	modifiedDate
	modifiedDate
	
	
	
	public String toString() {
		StringBuffer buffer = new StringBuffer();
		buffer.append(" userProfileId: " + userProfileId + ";");
		buffer.append(" displayName: " + displayName + ";");
		buffer.append(" firstName: " + displayName);
		return buffer.toString();
	}
	
}
