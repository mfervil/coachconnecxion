<?xml version="1.0" encoding="utf-8"?> 

<!DOCTYPE urlrewrite 

	PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" 
	"http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd"> 
	
<urlrewrite> 

	<rule> 
		<condition name="host" operator="notequal">^www\.</condition> 

		<condition name="host" operator="equal">^coachconnecxion\.com</condition> 
		<from>^(.*)$</from> 

		<to type="redirect">http://www.coachconnecxion.com$1</to> 
	</rule> 
</urlrewrite>